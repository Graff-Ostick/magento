class SimpleClass
{
    protected $var = " ";

    function displayVar(){
        $this->var = "protected";
        return $this->var;
    }
}

class Extend extends SimpleClass
{

    function displayVar()
    {
        $this->var = 'extend';
        return $this->var;
    }
}
$obj = new Extend();
echo $obj->displayVar();
